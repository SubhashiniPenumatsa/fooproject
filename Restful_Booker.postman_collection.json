				{
					"name": "Create new Booking",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"var jsonData = JSON.parse(responseBody);",
									"var status_code = 200;",
									"pm.test(\"Status code is \" + status_code, function () {",
									"    pm.response.to.have.status(status_code);",
									"});",
									"pm.test(\"Response time is less than 1000ms\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);",
									"});",
									"",
									"pm.globals.set(\"bookingID\", pm.response.json().bookingid);",
									"",
									"pm.test(\"Check firstname of new Booking\", function () {",
									"    pm.expect(jsonData.booking.firstname).to.eql(pm.environment.get(\"firstname\"));",
									"});   ",
									" pm.test(\"Check lastname of new Booking\", function () {    ",
									"    pm.expect(jsonData.booking.lastname).to.eql(pm.environment.get(\"lastname\"));",
									" });",
									" pm.test(\"Check totalprice of new Booking\", function () {  ",
									"    pm.expect(jsonData.booking.totalprice).to.eql(parseInt(pm.environment.get(\"totalprice\")));",
									" });",
									" pm.test(\"Check depositpaid of new Booking\", function () {   ",
									"    pm.expect(jsonData.booking.depositpaid).to.eql(true);",
									"});",
									"pm.test(\"Check checkin date\", function () {",
									"    pm.expect(jsonData.booking.bookingdates.checkin).to.eql(\"2020-03-07\");",
									"});",
									"pm.test(\"Check checkout date\", function () {  ",
									"    pm.expect(jsonData.booking.bookingdates.checkout).to.eql(\"2020-03-10\");",
									"});",
									"",
									"",
									"var url = pm.environment.get(\"url\") + \"/booking/\" +  pm.response.json().bookingid;",
									"",
									"const getRequest = {",
									"        url: url,",
									"        method: 'GET'",
									"};",
									"pm.sendRequest(getRequest, function (err, res) {",
									"    if (err) { ",
									"        console.log(err);",
									"    }",
									"    ",
									"    console.log(res);",
									"    ",
									"    pm.test(\"Response code 200\", function() {",
									"        pm.expect(res.code).to.equal(200);",
									"    });",
									"",
									"    pm.test(\"Check Total Price Matches\" , function () {",
									"    pm.expect(res.text()).to.include(\"230\");",
									"    });",
									"    pm.test(\"Body matches string\", function () {",
									"    pm.expect(res.text()).to.include(pm.environment.get(\"firstname\"));",
									"    pm.expect(res.text()).to.include(\"2020-03-07\");",
									"    })",
									"});",
									"",
									""
								],
								"type": "text/javascript"
							}
						},
